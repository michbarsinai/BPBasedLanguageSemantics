package bp.bflow.samples.lowlevel.forkjoinvalue;

import bp.bflow.samples.lowlevel.valueevents.StepStartEvent;


public class DStartRequest extends StepStartEvent {

	private Double a;
	private Double c1a;
	private Double c2;
	private Double c3;
	
	public DStartRequest() {
		super("D");
	}
	
	public DStartRequest(Double a, Double c1a,
			Double c2, Double c3) {
		super("D");
		this.a = a;
		this.c1a = c1a;
		this.c2 = c2;
		this.c3 = c3;
	}

	public Double getA() {
		return a;
	}

	public Double getC1a() {
		return c1a;
	}

	public Double getC2() {
		return c2;
	}

	public Double getC3() {
		return c3;
	}

}
